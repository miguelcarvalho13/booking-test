    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.11.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
